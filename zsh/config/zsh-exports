export EDTIOR=vim

path=("${HOME}/scripts" $path)

# OS-specific exports
case "$(uname -s)" in

Darwin)
	# Mac
  export JAVA_HOME="$(/usr/libexec/java_home -v17)"
  export ANDROID_HOME="$HOME/Library/Android/sdk"
	;;

Linux)
	;;

CYGWIN* | MINGW32* | MSYS* | MINGW*)
	# Windows
	;;
*)
	# Unknown
	;;
esac

export PATH

